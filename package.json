{
  "name": "folderr",
  "version": "2.0.0D",
  "description": "A secure open source host for images, and shortened urls..",
  "main": "dist/src/backend/index.js",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "vue-tsc --noEmit -p src/frontend && vite build",
    "build:backend": "tsc",
    "build:production": "tsc --build && vite build",
    "start": "node --enable-source-maps dist/src/backend/index.js",
    "setup": "node dist/scripts/setup.js",
    "dev": "ts-node-dev --trace-warnings -- src/backend/index.ts",
    "test": "eslint --ext .vue,.ts && node dist/src/backend/index.js",
    "lint": "eslint --ext .vue,.ts src/ scripts/ vite.config.ts",
    "lint:fix": "eslint --ext .vue,.ts --fix src/ scripts/ vite.config.ts",
    "installdev": "npm i -D && husky install"
  },
  "engines": {
    "node": ">=16.19.1 <=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Folderr/Folderr.git"
  },
  "author": "Folderr",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Folderr/Folderr/issues"
  },
  "homepage": "https://github.com/Folderr/Folderr#readme",
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/helmet": "^10.1.0",
    "@fastify/middie": "^8.1.0",
    "@fastify/multipart": "^7.5.0",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/static": "^6.9.0",
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^1.0.5",
    "@iconify/icons-codicon": "^1.2.13",
    "@iconify/vue": "^3.2.1",
    "@sentry/integrations": "^7.43.0",
    "@sentry/node": "^7.43.0",
    "@sentry/tracing": "^7.43.0",
    "@sentry/vue": "^7.43.0",
    "@tailwindcss/forms": "^0.5.3",
    "ajv": "^8.12.0",
    "argon2": "^0.40.3",
    "envalid": "^7.3.1",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "fldrr-web-sdk": "github:Folderr/fldrr-web-sdk",
    "got": "^11.8.2",
    "helmet": "^4.4.1",
    "js-yaml": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.29",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^7.6.3",
    "nodemailer": "^6.7.2",
    "pinia": "^2.2.1",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "spdy": "^4.0.2",
    "ulidx": "^2.3.0",
    "uuid": "^8.3.2",
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.0.1",
    "@iconify/types": "^1.1.0",
    "@types/busboy": "^0.3.1",
    "@types/connect": "^3.4.35",
    "@types/js-yaml": "^4.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mime-types": "^2.1.0",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^18.15.10",
    "@types/nodemailer": "^6.4.4",
    "@types/spdy": "^3.4.5",
    "@types/uuid": "^8.3.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-config-xo-typescript": "^5.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.0",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --ext .vue,.ts --fix",
    "*.{md,yaml}": "prettier --write"
  }
}
