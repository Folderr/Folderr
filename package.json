{
  "name": "folderr",
  "version": "2.0.0D",
  "description": "A secure open source host for images, and shortened urls..",
  "main": "src/index.js",
  "scripts": {
    "start": "node build/src/index.js",
    "first": "tsc && build/scripts/initFirst.js",
    "update": "echo \"updating\" && git pull && npm install && tsc && echo \"Update Success\"",
    "build": "tsc --skipLibCheck",
    "startpm2": "pm2 start --name folderr build/src/index.js",
    "lint": "eslint --ext .ts src scripts",
    "lintfix": "eslint --ext .ts --fix src scripts",
    "test": "NODE_ENV=test node -r esm src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Folderr/Folderr.git"
  },
  "author": "VoidNulll",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Folderr/Folderr/issues"
  },
  "homepage": "https://github.com/Folderr/Folderr#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/formidable": "^1.0.32",
    "@types/js-yaml": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime-types": "^2.1.0",
    "@types/moment-duration-format": "^2.2.2",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^13.13.4",
    "@types/nodemailer": "^6.4.0",
    "@types/spdy": "^3.4.4",
    "@types/superagent": "^4.1.10",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "helmet": "^4.4.1",
    "js-yaml": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.29",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.11.18",
    "nodemailer": "^6.5.0",
    "rate-limiter-flexible": "^2.2.1",
    "spdy": "^4.0.2",
    "superagent": "^6.1.0",
    "typescript": "^4.2.2",
    "winston": "^3.3.3"
