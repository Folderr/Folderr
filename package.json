{
  "name": "folderr",
  "version": "2.0.0D",
  "description": "A secure open source host for images, and shortened urls..",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "vue-tsc --noEmit -p src/frontend && vite build",
    "build:backend": "tsc --skipLibCheck",
    "start": "NODE_ENV=production node dist/src/backend/index.js",
    "start:plain": "node dist/src/backend/index.js",
    "setup": "node dist/scripts/setup.js",
    "dev": "NODE_ENV=dev DEBUG=true ts-node-dev src/backend/index.ts --trace-warnings",
    "dev:plain": "ts-node-dev src/backend/index.ts --trace-warnings",
    "test": "xo && NODE_ENV=test node dist/src/backend/index.js",
    "test:plain": "xo && node dist/src/backend/index.js",
    "prepare": "husky install",
    "lint": "xo src/ scripts/ vite.config.ts",
    "lint:fix": "xo --fix src/ scripts/ vite.config.ts"
  },
  "xo": {
    "plugins": [
      "optimize-regex",
      "radar"
    ],
    "extensions": [
      ".ts"
    ],
    "rules": {
      "indent": "off",
      "@typescript-eslint/indent": "off",
      "max-len": [
        "error",
        {
          "code": 100,
          "comments": 100,
          "ignoreUrls": true
        }
      ],
      "no-underscore-dangle": "error",
      "unicorn/prefer-node-protocol": "off"
    },
    "prettier": true
  },
  "engines": {
    "node": ">=12 <=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Folderr/Folderr.git"
  },
  "author": "VoidNulll",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Folderr/Folderr/issues"
  },
  "homepage": "https://github.com/Folderr/Folderr#readme",
  "dependencies": {
    "ajv": "^8.6.2",
    "argon2": "^0.28.2",
    "esm": "^3.2.25",
    "fastify": "^3.19.1",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-rate-limit": "^5.5.0",
    "fastify-static": "^4.2.2",
    "formidable": "^1.2.2",
    "got": "^11.8.2",
    "helmet": "^4.4.1",
    "js-yaml": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "middie": "^5.3.0",
    "mime-types": "^2.1.29",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.11.18",
    "nodemailer": "^6.5.0",
    "rate-limiter-flexible": "^2.2.1",
    "spdy": "^4.0.2",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.8",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@types/formidable": "^1.2.3",
    "@types/js-yaml": "^4.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mime-types": "^2.1.0",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^15.14.5",
    "@types/nodemailer": "^6.4.4",
    "@types/spdy": "^3.4.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.16.1",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.8",
    "autoprefixer": "^10.3.4",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-radar": "^0.2.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.6",
    "prettier": "2.3.1",
    "tailwindcss": "^2.2.9",
    "ts-node-dev": "^1.1.8",
    "vite": "^2.5.3",
    "vue-tsc": "^0.3.0",
    "xo": "^0.42.0"
  },
  "lint-staged": {
    "*.ts": "xo --fix",
    "*.{md,yaml}": "prettier --write"
  }
}
